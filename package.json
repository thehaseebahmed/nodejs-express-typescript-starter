{
  "name": "thehaseebahmed-nodejs-express-typescript-starter",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "npx ts-node --files -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@models": "dist/models",
    "@services": "dist/services",
    "@repos": "dist/repos",
    "@entities": "dist/entities",
    "@shared": "dist/shared",
    "@server": "dist/server",
    "@routes": "dist/routes",
    "@util": "dist/util"
  },
  "dependencies": {
    "@sentry/node": "^7.0.0",
    "@sentry/tracing": "^7.0.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "http-status-codes": "^2.2.0",
    "jade": "^0.31.2",
    "jet-logger": "^1.1.5",
    "module-alias": "^2.2.2",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.2",
    "@types/module-alias": "^2.0.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.36",
    "nodemon": "^2.0.16",
    "ts-node": "^10.8.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.2"
  }
}
